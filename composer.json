{
  "name": "tourze/wechat-official-account-jssdk-bundle",
  "description": "微信公众号JSSDK功能包",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "symfony/config": "^7.3",
    "symfony/dependency-injection": "^7.3",
    "symfony/framework-bundle": "^7.3",
    "symfony/http-foundation": "^7.3",
    "symfony/http-kernel": "^7.3",
    "symfony/routing": "^7.3",
    "symfony/yaml": "^7.3",
    "tourze/bundle-dependency": "1.*",
    "tourze/http-client-bundle": "0.1.*",
    "tourze/symfony-dependency-service-loader": "1.*",
    "tourze/symfony-routing-auto-loader-bundle": "0.0.*",
    "tourze/wechat-official-account-bundle": "0.1.*"
